        "createNewStorageAccount": {
			"value": true
        },
        "dataLakeAccount": {
            "value": "adlsprojectname"
        },
		"dataLakeAccountType":  {
            "value": "Standard_RAGRS"
        },
		"dataLakeAccountKind":   {
			"value": "StorageV2"
		},
        "createJumpVM": {
            "value": true
        },
        "virtualMachineName": {
            "value": "vm"
        },
        "vmAdminUsername": {
            "value": "cloudsa"
        },
        "vmAdminPassword": {
            "value": "ChangeThis"
        },
        "deployBastion": {
            "value": true
        },
		"bastionHostName": {
			"value": "bas-projectname"
		}

	}
